{
	"name": "minimal-express-mongoose-babel",
	"version": "1.0.0",
	"description": "boilerplate for an express mongoose and babel setup",
	"main": "index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"build":
			"rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
		"start": "npm run build && node dist/index.js"
	},
	"keywords": ["boilerplate", "express", "mongoose"],
	"author": "Musa Barighzaai",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.18.2",
		"express": "^4.16.2",
		"mongoose": "^4.13.0"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"rimraf": "^2.6.2"
	}
}
